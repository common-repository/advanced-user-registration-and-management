// =============================================================
// Form templates*/
// Removing this will remove all the form styling

.lr-ff-frame
	overflow hidden
	max-width 920px

.lr-pop-up-frame
	border 2px solid #000
	outline 10px solid rgba(0,0,0,0.5)

.lr-column-2
	float left
	padding 15px 30px
	margin 15px 0
	-webkit-box-sizing border-box
	-moz-box-sizing border-box
	box-sizing border-box

.lr-column-2.lr-input-group
	width 60%
	border-right 1px solid #eee
	position relative
	z-index 1

.lr-column-2.lr-input-group:before
	content "OR"
	display inline-block
	line-height 24px
	text-align center
	background #fff
	border 1px solid #eee
	border-radius 50%
	width 24px
	height 24px
	padding 4px
	position absolute
	right -18px
	top 50%
	margin-top -16px

.lr-column-2.lr-social-login-frame
	width 40%

.lr-column-2 .lr-ff-heading
	font-size 18px
	font-weight 700
	border-bottom 1px solid #ddd
	margin-bottom 15px
	padding 5px 0

.lr-column-2 .lr-ff-footnote
	padding 10px
	background-color #fafafa
	border 1px solid #eee
	font-size 13px

.lr-column-2 .lr-ff-left-right-frame
	overflow hidden
	text-align justify

.lr-column-2 .lr-ff-left-right-frameafter
	content ""
	display inline-block
	width 100%

.lr-column-2 .lr-ff-left-right-frame > *
	text-align left
	display inline-block
	vertical-align middle

.pull-right
	text-align right

.pull-left
	text-align left

.text-left
	position relative
	top -22px
	font-size 13px

#recaptcha_widget
	text-align left

.recaptcha_options
	margin-left 0px !important

.recaptcha_options li
	list-style-type none !important

#recaptcha_image span
	position relative
	bottom 15px

/*Horizontal Or Rule*/
.hr-or-rule
	position relative
	border-bottom 1px solid #ddd
	margin-bottom 30px
	padding-bottom 30px
	&:before
		content "OR"
		display inline-block
		line-height 24px
		text-align center
		background #fff
		border 1px solid #ddd
		border-radius 50%
		width 24px
		height 24px
		padding 4px
		position absolute
		margin-left -16px
		bottom -17px
		left 50%
		font-family sans-serif
		font-size 12px
		box-sizing initial
	&.vr
		border-left 1px solid #ddd
		margin-bottom 0
		border-bottom none
		&:before
			bottom auto
			left 0
			top 50%
			margin-top -16px

.loginradius-raas-validation-message, .popupvalidation-message
	color red

.interfacecontainerdiv
	overflow hidden
	max-width 500px
	margin 0 auto
	img
		border-radius 0
		box-shadow none
		display block
	li
		margin 0
		float left
		padding-right 5px
		list-style-type none
		margin-bottom 5px

.lr_linked_accounts
	border-top 1px solid #ddd
	clear both
	padding-top 10px

.messageinfo div
	padding 20px 30px

.lr-user-reg-container .messageinfo div
	padding 20px 0
.messageinfo .success
	color green
.messageinfo .error
	color red
.lr-social-provider-btn
	width calc(50% - 5px)
	margin-right 5px
	margin-bottom 5px
	float left
/*Icon Sprite*/
span[class*="lr-img-icon-"] 
	display inline-block
	/*if you change the width. don't forget to change the height and line height*/
	height 32px
	line-height 32px/*vertically centers it properly*/
	border-radius 2px
	font-size 13px
	&:before 
		content ''
		/*Data URI image*/
		background-image url('data:image/png;base64,')
		display inline-block
		width 24px
		height 24px
		background-size 100% auto
		vertical-align middle
	&.user-reg
		display block
		line-height 32px
		border-radius 2px
		padding 0 10px
		&:before 
			width 20px
			height 20px
		&:after 
			content attr(title)
			color #fff
			text-transform capitalize
			margin-left 10px

